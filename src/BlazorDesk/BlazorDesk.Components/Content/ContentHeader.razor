@namespace  BlazorDesk.Components
<!-- Content Header (Page header) -->
<div class="content-header">
	<div class="container-fluid">
		<div class="row mb-2">
			<div class="col-sm-6">
				<h1 class="m-0 text-dark">@Header</h1>
			</div><!-- /.col -->
			<div class="col-sm-6">
				@Breadcrum
			</div><!-- /.col -->
		</div><!-- /.row -->
	</div><!-- /.container-fluid -->
</div>
<!-- /.content-header -->
@code {
	[Parameter]
	public RenderFragment Header { get; set; }
	[Parameter]
	public RenderFragment Breadcrum { get; set; }
}
